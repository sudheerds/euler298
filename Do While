package test;

import java.util.Scanner;

public class Interactive {
	
	public static void main(String[] args){
		
		Scanner sc = new Scanner(System.in);
		int num;	
		
		do{	
		    System.out.println("Enter a number graeter than 10");			
			num=sc.nextInt();
		}while(num<10);
		
		System.out.println("Number greater than 10 detected");
			
			
		sc.close();
		
	}

}

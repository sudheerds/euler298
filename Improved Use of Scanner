package test;

import java.util.Scanner;

public class Interactive {
	
	public static void main(String[] args){
		
		Scanner sc = new Scanner(System.in);
		int num=0;
		
		
		do{
			System.out.println("Enter number 5 \n");
			
			if(sc.hasNextInt()){
				num=sc.nextInt();
			}
			
			else {
				System.out.println("You entered \t" + sc.nextLine()+"\n");
				
			}
			
		}while(num!=5);
		
		System.out.println("Got it");
			
		
		sc.close();
		
	}

}
